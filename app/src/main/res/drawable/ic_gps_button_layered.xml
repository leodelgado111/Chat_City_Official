<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="48dp"
    android:height="48dp"
    android:viewportWidth="48"
    android:viewportHeight="48">
    
    <!-- Layer 1: White Circle Background (Bottom) -->
    <path
        android:pathData="M24,24m-24,0a24,24 0,1 1,48 0a24,24 0,1 1,-48 0"
        android:fillColor="#FFFFFF" />
    
    <!-- Layer 2: Gradient Overlay (Middle) - 1dp smaller on each side for 1dp stroke -->
    <path
        android:pathData="M24,24m-23,0a23,23 0,1 1,46 0a23,23 0,1 1,-46 0">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="linear"
                android:angle="135"
                android:startColor="#97D4F0"
                android:centerColor="#8D93D0"
                android:endColor="#FB86BB" />
        </aapt:attr>
    </path>
    
    <!-- Layer 3: GPS Arrow pointing northeast (black) -->
    <!-- Arrow/triangle centered in 48x48 viewport, pointing to upper-right -->
    <path
        android:pathData="M16,32 L32,16 L32,32 Z"
        android:fillColor="#000000" />
        
</vector>